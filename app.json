        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Mr Driver Partner App to accesses your photos",
          "cameraPermission": "Kindly Allow Mr Driver Partner App to access your camera",
          "microphonePermission": "Allow Mr Driver Partner App to access your microphone"
        }
      ],
      [
        "expo-camera",
